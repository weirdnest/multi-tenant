{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@w7t/multi-tenant/(.*)": "<rootDir>/../libs/multi-tenant/src/$1",
    "@w7t/multi-tenant": "<rootDir>/../libs/multi-tenant/src"
  }
}